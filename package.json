{
  "name": "globi",
  "version": "0.0.64",
  "description": "embed EOL's Global Biotic Interactions Data http://github.com/jhpoelen/eol-globi-data/ in the browser",
  "main": "globi.js",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "d3": "~3.2.5",
    "globi-data": "~0.0.48",
    "jquery": ">=2.1",
    "taxaprisma": "0.0.11"
  },
  "devDependencies": {
    "tape": "~1.0.4"
  },
  "testling": {
    "files": "test/*.js",
        "browsers": [ "ie8", "ie9", "ff/13", "chrome/20" ]
  },

  "scripts": {
    "test": "testling",
