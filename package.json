  },

  "repository": {
    "type": "git",
    "url": "git://github.com/jhpoelen/eol-globi-js.git"
  },
  "keywords": [
    "eol",
    "species",
    "interactions",
    "bioinformatics",
    "browser",
    "ecology",
    "biodiversity"
  ],
