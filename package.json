{
    "name": "globi",
    "version": "0.0.49",
    "description": "embed EOL's Global Biotic Interactions Data http://github.com/jhpoelen/eol-globi-data/ in the browser",
    "main": "globi.js",
    "directories": {
        "example": "examples"
    },
    "dependencies": {
        "globi-data": "~0.0.40",
		    "d3": "~3.2.5",
        "jquery": ">=2.1"
    },
    "devDependencies": {
        "tape": "~1.0.4"
    },
    "testling": {
        "files": "test/*.js",
        "browsers": [ "ie8", "ie9", "ff/13", "chrome/20" ]
    },

    "scripts": {
        "test": "testling",
        "prepublish": "browserify . --standalone globi -o globi-dist.js"
    },

    "repository": {
        "type": "git",
        "url": "git://github.com/jhpoelen/eol-globi-js.git"
    },
    "keywords": [
        "eol",
        "species",
        "interactions",
        "bioinformatics",
        "browser",
        "ecology",
        "biodiversity"
    ],
    "author": "Jorrit Poelen",
